<style>
  #dropdown { height: 200px; }
</style>




<div class="create_parking_page">
  <%= simple_form_for @space, :html=> { class: 'foobarbaz' } do |f| %>
    <%= f.input :address, :input_html => {
          :id => "location-input"
        }%>
    <%= f.input :price, :input_html => {
          :id => "location-input"
        }%>
    <%= f.input :parking_type, collection: ["garage", "public drive", "street", "private drive"], :id => 'dropdown'%>
    <%= f.input :photo %>
    <%= f.submit 'CREATE', :class => 'btn btn-success'%>
  <% end %>
</div>

